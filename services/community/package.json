{
  "name": "community-service",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "push": "drizzle-kit push",
    "generate": "drizzle-kit generate",
    "migrate": "tsx src/db/migrate.ts",
    "studio": "drizzle-kit studio --port=4988",
    "test": "vitest run",
    "test:unit": "vitest run --reporter=verbose",
    "test:watch": "vitest watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@common/env": "workspace:*",
    "@common/events": "workspace:*",
    "@common/http": "workspace:*",
    "@common/logger": "workspace:*",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.45.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "postgres": "^3.4.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.22.4",
    "amqplib": "^0.10.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/pg": "^8.10.9",
    "@types/amqplib": "^0.10.4",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.6",
    "@vitest/ui": "^2.1.8",
    "drizzle-kit": "^0.31.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^2.1.8"
  }
}
